{
    "name": "rs/cms",
    "version": "0.9.44",
    "description": "The Reverse Systems CMS.",
    "keywords": ["laravel", "cms"],
    "license": "no license",
    "authors": [
        {
            "name": "Zakharov Dmitry",
            "email": "d.zakharov@reverse.systems"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "fideloper/proxy": "^4.0",
        "kalnoy/nestedset": "^4.3",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.4.0",
        "santigarcor/laratrust": "5.0.*",
        "unisharp/laravel-filemanager": "~1.8"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\Cms\\": "app/Cms/"
        }
    },
    "extra": {
      "laravel": {
        "providers": [
          "App\\Cms\\Providers\\CmsModulesServiceProvider",
          "App\\Cms\\Providers\\CmsServiceProvider"
        ]
      }
    }
}
